{
	"name": "@intrnl/svelte-query",
	"description": "Svelte library for managing, caching and syncing asynchronous and remote data",
	"version": "0.3.1",
	"author": "intrnl",
	"license": "MIT",
	"source": "lib/index.ts",
	"main": "dist/svelte-query.js",
	"module": "dist/svelte-query.mjs",
	"types": "types/index.d.ts",
	"sideEffects": false,
	"files": [
		"dist/",
		"types/",
		"README.md",
		"LICENSE"
